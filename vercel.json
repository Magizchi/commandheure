{
    "version":2,
    "framework": "node",
    "github":{
        "enable": false
    },
    "builds": [
        {
            "src":"/apps/client/dist",
            "use":"@vercel/node"
        }, 
        {
            "src":"/apps/api/dist",
            "use":"@now/static"
        }
    ],
    "routes":[
        {"src": "/(.*)", "dest":"app/api/src/main.ts", "methods":["GET","POST","PUT"]}
    ]
}