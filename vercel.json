{
    "version":2,
    "github":{
        "enable": false
    },
    "build": [
        {
            "src":"/apps/client/dist",
            "use":"@vercel/node"
        }, 
        {
            "src":"/apps/api/dist",
            "use":"@now/static"
        }
    ],
    "routes":[
        {"src": "./(.*)", "dest":"app/api/src/main.ts", "method":["GET"]}
    ]
}